<layout tools:viewBindingIgnore="true"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <import type="com.cavss.artravel.ui.custom.recyclerview.IClickListener"/>
        <variable
            name="model"
            type="com.cavss.artravel.ui.view.screen.setting.AuthIconModel" />
        <variable
            name="clickCallback"
            type="IClickListener" />
        <variable
            name="position"
            type="Integer" />
    </data>

    <ImageView
        app:img="@{model.image}"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_margin="5dp"
        android:padding="10dp"
        android:background="@drawable/bg_round"
        android:scaleType="centerCrop"
        android:onClick="@{() -> clickCallback.onItemClick(model, position)}">
    </ImageView>
</layout>